  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
